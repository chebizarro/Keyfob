{
  "manifest_version": 3,
  "name": "Keyfob NIP-07",
  "version": "0.1.0",
  "description": "Injects window.nostr provider and bridges to native Keyfob.",
  "permissions": [],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
